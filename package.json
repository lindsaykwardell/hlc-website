    "cssnano": "^4.1.10",
    "node-sass": "^4.13.0",
    "polka": "next",
    "postcss-cli": "^6.1.3",
    "postcss-nested": "^4.2.1",
    "serve": "^11.2.0",
    "sirv": "^0.4.0",
    "svelte-preprocess": "^3.2.6",
    "svelte-ts-preprocess": "^1.1.3",
    "tailwindcss": "^1.1.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
