{
  "name": "hlc-website",
  "description": "The website for Healthy Living Community",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build:css": "postcss index.postcss -o static/global.css",
    "watch:css": "postcss index.postcss -o static/global.css -w",
